#ONU CDATA-FD514GD-R460 Mode PPPoE

#buat vlan
conf t
vlan 262
name vlan_262_NEO_SMKN_2_Boyolangu

#konfigurasi vlan tag
conf t
int xgei_1/3/2
switchport vlan 262 tag

#membuat profile vlan
conf t
gpon
onu profile vlan NEO_SMKN_2_Boyolangu tag-mode tag cvlan 262 pri 7
end
write

#mendaftarkan ONU
conf t
interface gpon-olt_1/2/12
onu 3 type CDATA-FD514GD-R460 sn ZTEG1D09343F
exit

#konfigurasi
interface gpon-onu_1/2/12:3
name NEO_SMKN_2_Boyolangu
description NEO_SMKN_2_Boyolangu
sn-bind enable sn
tcont 1 name PPPOE profile 100M
gemport 1 name PPPOE unicast tcont 1 dir both
gemport 1 traffic-limit upstream 100M downstream 100M
switchport mode hybrid vport 1
service-port 1 vport 1 user-vlan 262 vlan 262
port-location format dsl-forum sport 1
port-location sub-option remote-id enable sport 1
port-location sub-option remote-id name 17 sport 1
port-location ft-cid 17 sport 1
pppoe-plus enable sport 1
pppoe-plus trust true replace sport 1
exit

#konfigurasi
pon-onu-mng gpon-onu_1/2/12:3
service PPPOE type internet gemport 1 vlan 262
wan-ip 1 mode pppoe username smkn_2_boyolangu password 5758 vlan-profile NEO_SMKN_2_Boyolangu host 1
wan-ip 1 ping-response enable traceroute-response enable
security-mng 1 state enable mode permit protocol web
exit
end

#jika sudah yakin benar
write 

#reset onu
conf t
pon-onu-mng gpon-onu_1/2/12:3
restore factory
