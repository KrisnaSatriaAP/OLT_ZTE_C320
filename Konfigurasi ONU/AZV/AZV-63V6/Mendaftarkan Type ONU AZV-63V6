#mendaftarkan

conf t
pon
onu-type AZV-63V6 gpon description 1GE+3FE+1POTS+WIFI
onu-type AZV-63V6 gpon max-tcont 7 max-gemport 32 max-switch-perslot 8 max-flow-perswitch 32 max-iphost 5
onu-type-if AZV-63V6 eth_0/1-4
onu-type-if AZV-63V6 pots_0/1
onu-type-if AZV-63V6 wifi_0/1
end
write

#cek apa sudah masuk
sho onu-type gpon AZV-63V6
